list_E = [0, 2, 4, 6, 8]
list_N = [1, 2, 3, 4, 5]

set_E = set(list_E)
set_N = set(list_N)

union_result = list(set_E.union(set_N))
intersection_result = list(set_E.intersection(set_N))
difference_result = list(set_E.difference(set_N))
symmetric_difference_result = list(set_E.symmetric_difference(set_N))

union_result = tuple(union_result)
intersection_result = tuple(intersection_result)
difference_result = tuple(difference_result)
symmetric_difference_result = tuple(symmetric_difference_result)

print(f"Union of E and N is {union_result}")
print(f"Intersection of E and N is {intersection_result}")
print(f"Difference of E and N is {difference_result}")
print(f"Symmetric difference of E and N is {symmetric_difference_result}")
